<!--
parametrization of a 2D Gaussian fitted from the 68% CL contour
in the plane mu(ggH, gamma gamma) vs. mu(VBF+VH, gamma gamma)
with profiled Higgs mass (!), shown in CMS-HIG-16-040
http://cms-results.web.cern.ch/cms-results/public-results/preliminary-results/HIG-16-040/index.html
-->

<expmu decay="gammagamma" dim="2" type="n">
  <experiment>CMS</experiment>
  <source type="preliminary">HIG-16-040</source>
  <sqrts>13</sqrts>
  <mass>125.09</mass>
  <CL>68%</CL> <!-- optional -->

  <eff axis="x" prod="ggH">1.0</eff>
  <eff axis="y" prod="VVH">1.0</eff>

  <bestfit>
    <x>1.207</x>
    <y>1.07</y>
  </bestfit>
  
  <param>
    <a>32.96</a>
    <b>4.894</b>
    <c>4.143</c>
  </param>
</expmu>
